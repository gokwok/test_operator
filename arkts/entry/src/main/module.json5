{
  "module": {
    "name": "entry",
    "type": "entry",
    "description": "$string:module_desc",
    "mainElement": "EntryAbility",
    "deviceTypes": [
      "phone",
      "tablet",
      "2in1"
    ],
    "deliveryWithInstall": true,
    "installationFree": false,
    "pages": "$profile:main_pages",
    "requestPermissions": [
      {
        "name": "ohos.permission.INTERNET"
      },
      {
        "name": "ohos.permission.ACCESS_VIRTUAL_SCREEN"
      },
      {
        "name": "ohos.permission.CAPTURE_SCREEN"
      },
      {
        "name": "ohos.permission.SYSTEM_FLOAT_WINDOW"
      },
      {
        "name": "ohos.permission.START_ABILITIES_FROM_BACKGROUND"
      },
      {
        "name": "ohos.permission.START_INVISIBLE_ABILITY"
      },
      {
        "name": "ohos.permission.GET_BUNDLE_INFO_PRIVILEGED"
      },
      {
        "name": "ohos.permission.GET_BUNDLE_RESOURCES"
      },
      {
        "name": "ohos.permission.GET_INSTALLED_BUNDLE_LIST",
        "reason": "$string:permission_get_installed_bundle_list_reason",
        "usedScene": {
          "abilities": [
            "EntryAbility",
            "AgentServiceExtAbility"
          ],
          "when": "always"
        }
      },
      {
        "name": "ohos.permission.WRITE_IMAGEVIDEO",
        "reason": "$string:permission_write_imagevideo_reason",
        "usedScene": {
          "abilities": [
            "EntryAbility",
            "AgentServiceExtAbility"
          ],
          "when": "inuse"
        }
      },
      {
        "name": "ohos.permission.MICROPHONE",
        "reason": "$string:permission_microphone_reason",
        "usedScene": {
          "abilities": [
            "EntryAbility",
            "AgentServiceExtAbility"
          ],
          "when": "inuse"
        }
      },
      {
        "name": "ohos.permission.GET_WALLPAPER",
        "reason": "$string:permission_get_wallpaper_reason",
        "usedScene": {
          "abilities": [
            "EntryAbility",
            "AgentServiceExtAbility",
            "ScreenboardAbility"
          ],
          "when": "inuse"
        }
      }
    ],
    "abilities": [
      {
        "name": "EntryAbility",
        "srcEntry": "./ets/abilities/EntryAbility.ets",
        "description": "$string:EntryAbility_desc",
        "icon": "$media:startIcon",
        "label": "$string:EntryAbility_label",
        "startWindowIcon": "$media:startIcon",
        "startWindowBackground": "$color:start_window_background",
        "exported": true,
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "ohos.want.action.home"
            ]
          }
        ]
      },
      {
        "name": "AgentSpaceViewerAbility",
        "srcEntry": "./ets/abilities/AgentSpaceViewerAbility.ets",
        "description": "$string:EntryAbility_desc",
        "icon": "$media:layered_image",
        "label": "$string:EntryAbility_label",
        "startWindowIcon": "$media:startIcon",
        "startWindowBackground": "$color:start_window_background",
        "exported": false,
        "launchType": "singleton",
        "removeMissionAfterTerminate": true
      },
      {
        "name": "ScreenboardAbility",
        "srcEntry": "./ets/abilities/ScreenboardAbility.ets",
        "description": "$string:EntryAbility_desc",
        "icon": "$media:layered_image",
        "label": "$string:EntryAbility_label",
        "startWindowIcon": "$media:startIcon",
        "startWindowBackground": "$color:start_window_background",
        "exported": false,
        "launchType": "singleton",
        "removeMissionAfterTerminate": true
      }
    ],
    "extensionAbilities": [
      {
        "name": "EntryBackupAbility",
        "srcEntry": "./ets/abilities/EntryBackupAbility.ets",
        "type": "backup",
        "exported": false,
        "metadata": [
          {
            "name": "ohos.extension.backup",
            "resource": "$profile:backup_config"
          }
        ]
      },
      {
        "name": "AgentServiceExtAbility",
        "srcEntry": "./ets/abilities/AgentServiceExtAbility.ets",
        "type": "service",
        "exported": false
      },
      {
        "name": "FloatWindowAbility",
        "srcEntry": "./ets/abilities/FloatWindowAbility.ets",
        "type": "service",
        "exported": true
      }
    ]
  }
}
